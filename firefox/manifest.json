{
    "name": "Auto Theater Mode",
    "description": "Extension to automatically open youtube videos in theater Mode",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "scripting",
        "tabs",
        "https://www.youtube.com/watch*"
    ]
}